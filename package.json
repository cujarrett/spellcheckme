{
  "name": "spellcheckme",
  "version": "0.5.13",
  "description": "Spellcheck CLI to quickly check the spelling of a word or two using the power of Google's search engine via a headless browser.",
  "main": "./src/index.js",
  "bin": "./src/index.js",
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/cujarrett/spellcheckme.git"
  },
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint .",
    "fix-lint": "eslint . --fix",
    "test": "nyc ava",
    "ci": "npm run lint && npm run test",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "contributors": [
    {
      "name": "Matt Jarrett",
      "web": "https://mattjarrett.dev"
    }
  ],
  "keywords": [
    "spellcheck",
    "cli"
  ],
  "author": "Matt Jarrett",
  "license": "MIT",
  "dependencies": {
    "chalk": "~4.1.2",
    "puppeteer": "~10.2.0",
    "update-notifier": "~5.1.0",
    "yargs": "~17.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "~13.1.0",
    "@commitlint/config-conventional": "~13.1.0",
    "@semantic-release/changelog": "~5.0.1",
    "@semantic-release/commit-analyzer": "~8.0.1",
    "@semantic-release/exec": "~5.0.0",
    "@semantic-release/git": "~10.0.0",
    "@semantic-release/github": "~7.2.3",
    "@semantic-release/npm": "~7.1.3",
    "@semantic-release/release-notes-generator": "~10.0.2",
    "ava": "~3.15.0",
    "commitizen": "~4.2.4",
    "cz-conventional-changelog": "~3.3.0",
    "eslint": "~7.32.0",
    "husky": "~7.0.2",
    "nyc": "~15.1.0",
    "semantic-release": "~18.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}

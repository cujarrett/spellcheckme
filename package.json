{
  "name": "spellcheckme",
  "version": "1.1.2",
  "license": "MIT",
  "author": "Matt Jarrett",
  "description": "Spellcheck CLI to quickly check the spelling of a word or two using the power of Google's search engine via a headless browser.",
  "type": "module",
  "exports": "./index.js",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "main": "./src/index.js",
  "bin": "./src/index.js",
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/cujarrett/spellcheckme.git"
  },
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint .",
    "fix-lint": "eslint . --fix",
    "test": "c8 ava",
    "ci": "npm run lint && npm run test",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "contributors": [
    {
      "name": "Matt Jarrett",
      "web": "https://mattjarrett.dev"
    }
  ],
  "keywords": [
    "spellcheck",
    "cli"
  ],
  "dependencies": {
    "chalk": "~5.0.0",
    "puppeteer": "~19.2.2",
    "update-notifier": "~5.1.0",
    "yargs": "~17.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "~15.0.0",
    "@commitlint/config-conventional": "~15.0.0",
    "@semantic-release/changelog": "~6.0.1",
    "@semantic-release/commit-analyzer": "~9.0.2",
    "@semantic-release/exec": "~6.0.2",
    "@semantic-release/git": "~10.0.1",
    "@semantic-release/github": "~8.0.2",
    "@semantic-release/npm": "~8.0.3",
    "@semantic-release/release-notes-generator": "~10.0.3",
    "ava": "~3.15.0",
    "c8": "~7.10.0",
    "commitizen": "~4.2.4",
    "cz-conventional-changelog": "~3.3.0",
    "eslint": "~8.3.0",
    "husky": "~7.0.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
